    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.11",
    "@types/nodemailer": "^6.4.15",
    "@types/nodemailer-express-handlebars": "^4.0.5",
    "@types/swagger-ui-express": "^4.1.6",
    "@typespec/compiler": "^0.56.0",
    "@typespec/http": "^0.56.0",
    "@typespec/rest": "^0.56.0",
