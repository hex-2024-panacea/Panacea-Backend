    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "~4.16.1",
    "express-handlebars": "^7.1.2",
    "express-rate-limit": "^7.2.0",
    "express-throttle": "^2.0.0",
    "googleapis": "^137.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.3",
    "morgan": "~1.9.1",
    "nodemailer": "^6.9.13",
    "nodemailer-express-handlebars": "^6.1.2",
    "swagger-autogen": "^2.23.7",
