{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc --project ./",
    "lint": "eslint .",
    "start": "node ./bin/www",
    "dev": "tsc -w & nodemon ./bin/www",
    "build-typespec": "tsp compile ./spec/main.tsp --output-dir ./spec",
    "build-swagger": "swagger-cli bundle ./spec/@typespec/openapi3/openapi.yaml -t json -o ./spec/swagger.json",
    "build-docs": "tsp compile ./spec/main.tsp --output-dir ./spec && swagger-cli bundle ./spec/@typespec/openapi3/openapi.yaml -t json -o ./spec/swagger.json"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/luxon": "^3.4.2",
    "@types/nodemailer": "^6.4.15",
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "~4.16.1",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.4.4",
    "mongoose": "^8.3.4",
    "morgan": "~1.9.1",
    "nodemailer": "^6.9.13",
    "swagger-ui-express": "^5.0.0",
    "typescript-eslint": "^7.8.0",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.10",
    "@typespec/compiler": "latest",
    "@typespec/http": "latest",
    "@typespec/openapi3": "latest",
    "@typespec/rest": "latest",
    "eslint": "^9.2.0",
    "globals": "^15.1.0",
    "nodemon": "^3.1.0",
    "swagger-cli": "^4.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
